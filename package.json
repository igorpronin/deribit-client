{
  "name": "@igorpronin/deribit-client",
  "version": "1.1.2",
  "description": "Deribit client for internal purposes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "clear": "rm -rf ./dist",
    "dev": "nodemon ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stats": "./tools/git-daily-stats.sh",
    "permit": "chmod +x ./tools/update-version.sh && chmod +x ./tools/git-commit.sh && chmod +x ./tools/release.sh && chmod +x ./tools/build-and-publish.sh && chmod +x ./tools/git-daily-stats.sh",
    "release:patch": "npm run clear && ./tools/release.sh patch",
    "release:minor": "npm run clear && ./tools/release.sh minor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorpronin/deribit-client.git"
  },
  "keywords": [],
  "author": "legalsolut@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/node": "^20.11.26",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@types/ws": "^8.5.10",
    "@igorpronin/utils": "^1.0.4",
    "events": "^3.3.0",
    "ws": "^8.16.0"
  }
}
